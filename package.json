{
  "name": "super-workers",
  "version": "0.1.0",
  "description": "Distribute load on front-end via parallelism",
  "main": "dist/super-workers.js",
  "scripts": {
    "build": "parallel-webpack --mode=build",
    "build-dev": "parallel-webpack -- webpack --mode=dev",
    "build-test": "karma start --single-run --no-auto-watch karma.config.js",
    "dev": "parallel-webpack -- webpack --progress --colors --watch --mode=dev",
    "test": "karma start karma.config.js",
    "coverage": "node node_modules/.bin/istanbul cover karma start --single-run karma.config.js",
    "coveralls": "cat ./coverage/report-lcov/lcov.info | node node_modules/.bin/coveralls"
  },
  "devDependencies": {
    "babel": "6.3.13",
    "babel-core": "6.1.18",
    "babel-eslint": "5.0.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-add-module-exports": "0.1.2",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-preset-es2015": "6.3.13",
    "coveralls": "^2.11.16",
    "eslint": "1.7.2",
    "eslint-loader": "1.1.0",
    "isparta-loader": "^2.0.0",
    "istanbul": "^1.0.0-alpha.2",
    "jasmine": "^2.5.3",
    "jasmine-core": "^2.5.2",
    "karma": "^1.4.1",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.1.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-webpack": "^2.0.2",
    "parallel-webpack": "^1.6.1",
    "webpack": "1.12.9",
    "yargs": "3.32.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/softvar/super-workers.git"
  },
  "keywords": [
    "super-workers",
    "web worker",
    "worker postmessage",
    "distrubte tasks",
    "load balancing",
    "es6",
    "commonjs",
    "webpack"
  ],
  "author": "Varun Malhotra",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/softvar/super-workers/issues"
  },
  "homepage": "https://github.com/softvar/super-workers",
  "dependencies": {}
}
